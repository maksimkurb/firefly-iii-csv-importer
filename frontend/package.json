{
  "name": "firefly-importer",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint ./ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --ignore-path .eslintignore",
    "openapi:generate": "openapi-generator-cli generate -g typescript-axios -i ../specs/firefly-iii-importer.json -o src/api/"
  },
  "dependencies": {
    "@inkline/inkline": "^3.1.9",
    "axios": "^1.2.0",
    "monaco-editor": "^0.34.1",
    "papaparse": "^5.3.2",
    "pinia": "^2.0.27",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.10",
    "@types/papaparse": "^5.3.5",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.2.5",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^11.2.0",
    "eslint": "^8.29.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.19",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.0",
    "sass": "^1.56.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^0.25.3",
    "vue-tsc": "^1.0.11"
  }
}
