{
  "name": "firefly-importer",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint ./ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --ignore-path .eslintignore",
    "openapi:generate": "openapi-generator-cli generate -g typescript-axios -i ../specs/firefly-iii-importer.json -o src/api/"
  },
  "dependencies": {
    "@inkline/inkline": "^3.0.8",
    "axios": "^0.27.2",
    "monaco-editor": "^0.34.0",
    "papaparse": "^5.3.2",
    "pinia": "^2.0.13",
    "vue": "^3.2.39",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@types/papaparse": "^5.3.5",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^9.5.4",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.16",
    "postcss-preset-env": "^7.8.2",
    "prettier": "^2.5.1",
    "sass": "^1.52.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.9.3",
    "vite": "^2.9.5",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  }
}
